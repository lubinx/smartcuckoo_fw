  "configurations": [
    {
      "name": "ultracore: arm-none-eabi",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/ultracore/inc",
        "${workspaceFolder}/efr32xg22/include"
      ],
      "defines": [
        "PANEL_B",
        "PANEL_C"
